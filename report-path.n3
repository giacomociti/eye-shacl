
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix this: <http://eye-shacl#> .

{ ?node this:has ?node } <= { } .

{ ?node this:has ?child } <= { ?node list:member ?child } .

{
    ?node this:has ?child
}
<=
{
    ?p list:in (
        sh:inversePath
        sh:alternativePath
        sh:zeroOrMorePath
        sh:oneOrMorePath
        sh:zeroOrOnePath
    ).
    ?node ?p ?child .
} .

{
    ?node this:has ?child
}
<=
{
    ?p list:in (
        sh:inversePath
        sh:alternativePath
        sh:zeroOrMorePath
        sh:oneOrMorePath
        sh:zeroOrOnePath
    ).
    ?node ?p ?o .
    ?o this:has ?child .
} .

{
    ?node this:has ?child
}
<=
{
    ?node sh:alternativePath [ list:member ?child ]
} .



