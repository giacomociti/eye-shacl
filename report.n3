@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix list: <http://www.w3.org/2000/10/swap/list#> .

{ ?report a sh:ValidationReport ; ?p ?o } => { ?report ?p ?o } .

{ 
    ?report sh:result ?result . 
    ?result ?p ?o .
} 
=> 
{ 
    ?result ?p ?o 
} .

{
    ?result sh:resultPath ?resultPath .
    ?resultPath ?p ?o .
    ?p list:in (
        sh:inversePath
        sh:alternativePath
        sh:zeroOrMorePath
        sh:oneOrMorePath
        sh:zeroOrOnePath
    )
}
=>
{
    ?resultPath ?p ?o .
} .
