# https://www.w3.org/TR/shacl/#core-components-range

@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix this: <http://eye-shacl#> .
@prefix pred: <http://www.w3.org/2007/rif-builtin-predicate#>.

{ ?value this:minExclusive ?minExclusive } <= { ?value a this:Number ; math:greaterThan ?minExclusive }.
{ ?value this:minInclusive ?minInclusive } <= { ?value a this:Number ; math:notLessThan ?minInclusive }.
{ ?value this:maxExclusive ?maxExclusive } <= { ?value a this:Number ; math:lessThan ?maxExclusive }.
{ ?value this:maxInclusive ?maxInclusive } <= { ?value a this:Number ; math:notGreaterThan ?maxInclusive }.

{ ?value this:minExclusive ?minExclusive } <= { ?value this:datatype xsd:string ; string:greaterThan ?minExclusive }.
{ ?value this:minInclusive ?minInclusive } <= { ?value this:datatype xsd:string ; string:notLessThan ?minInclusive }.
{ ?value this:maxExclusive ?maxExclusive } <= { ?value this:datatype xsd:string ; string:lessThan ?maxExclusive }.
{ ?value this:maxInclusive ?maxInclusive } <= { ?value this:datatype xsd:string ; string:notGreaterThan ?maxInclusive }.

{ ?value this:minExclusive ?minExclusive } <= { ?value this:datatype xsd:dateTime . (?value ?minExclusive) pred:dateTime-greater-than true }.
{ ?value this:minInclusive ?minInclusive } <= { ?value this:datatype xsd:dateTime . (?value ?minInclusive) pred:dateTime-greater-than-or-equal true }.
{ ?value this:maxExclusive ?maxExclusive } <= { ?value this:datatype xsd:dateTime . (?value ?maxExclusive) pred:dateTime-less-than true }.
{ ?value this:maxInclusive ?maxInclusive } <= { ?value this:datatype xsd:dateTime . (?value ?maxInclusive) pred:dateTime-less-than-or-equal true }.

{ ?value a this:Number } <= { ?value this:datatype [ list:in (xsd:integer xsd:decimal xsd:double xsd:float) ] }.

